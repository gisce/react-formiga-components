{
  "name": "@gisce/react-formiga-components",
  "description": "A React UI component library for building forms based in Ant Design",
  "author": {
    "name": "GISCE-TI, S.L",
    "url": "https://gisce.net/"
  },
  "license": "MIT",
  "keywords": [
    "react",
    "form",
    "antd",
    "formiga",
    "components",
    "ui",
    "storybook",
    "library"
  ],
  "packageManager": "npm@9.8.0",
  "engines": {
    "node": "20.5.0"
  },
  "version": "1.2.0-alpha.6",
  "module": "./dist/react-formiga-components.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/react-formiga-components.es.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build:tsc": "tsc -p tsconfig.build.json --noEmit",
    "build:vite": "vite build",
    "build": "concurrently npm:build:*",
    "dev:vite": "npm run build:vite -- --watch --force",
    "dev:tsc": "npm run build:tsc -- --watch",
    "prepare": "husky install",
    "start": "concurrently npm:dev:*",
    "check": "lint-staged",
    "analyze": "vite-bundle-visualizer",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "antd": "5.13.1",
    "classnames": "^2.5.1",
    "file-type-buffer-browser": "git+ssh://git@github.com/mguellsegarra/file-type-buffer-browser.git",
    "interweave": "^13.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "styled-components": "5.3.5",
    "use-deep-compare-effect": "^1.8.1",
    "validator": "^13.6.0"
  },
  "peerDependencies": {
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "styled-components": "5.3.5"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.3",
    "@gisce/commitlint-rules": "1.0.5",
    "@semantic-release/exec": "6.0.3",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/npm": "10.0.4",
    "@storybook/addon-essentials": "^7.6.8",
    "@storybook/addon-interactions": "^7.6.8",
    "@storybook/addon-links": "^7.6.8",
    "@storybook/addon-onboarding": "^1.0.10",
    "@storybook/blocks": "^7.6.8",
    "@storybook/react": "^7.6.8",
    "@storybook/react-vite": "^7.6.8",
    "@storybook/test": "^7.6.8",
    "@types/lodash": "^4.14.170",
    "@types/react": "18.2.18",
    "@types/react-dom": "18.2.7",
    "@types/rollup-plugin-peer-deps-external": "^2.2.2",
    "@types/styled-components": "^5.1.7",
    "@types/validator": "^13.6.3",
    "@typescript-eslint/eslint-plugin": "5.59.0",
    "@typescript-eslint/parser": "5.59.0",
    "@vitejs/plugin-react-swc": "3.3.2",
    "concurrently": "8.2.0",
    "eslint": "8.51.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-config-standard-with-typescript": "24.0.0",
    "eslint-plugin-import": "2.28.0",
    "eslint-plugin-n": "15.6.0",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-react": "7.33.1",
    "eslint-plugin-react-hooks": "4.6.0",
    "husky": "8.0.3",
    "lint-staged": "13.2.3",
    "prettier": "3.0.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "semantic-release": "21.0.7",
    "storybook": "^7.6.8",
    "typescript": "^5.0.4",
    "vite": "^4.5.1",
    "vite-bundle-visualizer": "^0.10.0",
    "vite-plugin-dts": "^3.6.0",
    "vite-tsconfig-paths": "3.5.0"
  },
  "overrides": {
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": "eslint",
    "src/**/*.{js,jsx,ts,tsx,json,css,md}": "prettier --write"
  },
  "release": {
    "branches": [
      "main",
      {
        "name": "develop",
        "prerelease": "rc"
      },
      {
        "name": "alpha",
        "prerelease": true
      }
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/exec",
        {
          "prepareCmd": "npm run build"
        }
      ],
      [
        "@semantic-release/npm",
        {
          "npmPublish": true
        }
      ],
      "@semantic-release/github",
      "@semantic-release/git"
    ]
  }
}
