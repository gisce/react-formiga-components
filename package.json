{
  "name": "@gisce/react-formiga-components",
  "description": "A React UI component library for building forms based in Ant Design",
  "author": {
    "name": "GISCE-TI, S.L",
    "url": "https://gisce.net/"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/gisce/react-formiga-components/"
  },
  "keywords": [
    "react",
    "form",
    "antd",
    "formiga",
    "components",
    "ui",
    "storybook",
    "library"
  ],
  "version": "1.0.9-rc.2",
  "main": "./dist/react-formiga-components.umd.js",
  "module": "./dist/react-formiga-components.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/react-formiga-components.es.js",
      "require": "./dist/react-formiga-components.umd.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "preminor": "npm version preminor --preid rc",
    "prerelease": "npm version prerelease --preid rc",
    "prepatch": "npm version prepatch --preid rc",
    "premajor": "npm version premajor --preid rc",
    "start": "while :; do vite build --watch; done",
    "build": "vite build",
    "prebuild:storybook": "npm run test:generate-output",
    "build:storybook": "build-storybook -c .storybook -o storybook-static/",
    "storybook": "start-storybook -p 6006",
    "test": "jest",
    "test:generate-output": "jest --json --outputFile=.jest-test-results.json",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "antd": "^4.17.2",
    "file-type-buffer-browser": "git+ssh://git@github.com/mguellsegarra/file-type-buffer-browser.git",
    "moment": "^2.29.1",
    "validator": "^13.7.0"
  },
  "peerDependencies": {
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "styled-components": "^5.3.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.0.6",
    "@storybook/addon-actions": "^6.4.1",
    "@storybook/addon-docs": "^6.4.0-rc.11",
    "@storybook/addon-essentials": "^6.4.1",
    "@storybook/addon-jest": "^6.4.9",
    "@storybook/addon-links": "^6.4.1",
    "@storybook/builder-webpack5": "^6.4.0",
    "@storybook/manager-webpack5": "^6.4.0",
    "@storybook/node-logger": "^6.4.0",
    "@storybook/react": "^6.4.1",
    "@storybook/testing-react": "^1.2.2",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@types/jest": "^27.0.3",
    "@types/node": "^12.20.37",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "@types/react-grid-layout": "^1.3.2",
    "@types/styled-components": "^5.1.15",
    "@types/validator": "^13.7.6",
    "babel-loader": "^8.2.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.4.3",
    "jest-styled-components": "^7.0.8",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "rimraf": "^3.0.2",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-terser": "^7.0.2",
    "ts-jest": "^27.1.1",
    "typescript": "^4.5.2",
    "webpack": "^5.14.0",
    "webpack-bundle-analyzer": "^4.3.0",
    "webpack-cli": "^4.3.1",
    "vite": "^2.8.6",
    "vite-plugin-dts": "^1.0.3",
    "@vitejs/plugin-react": "^1.2.0"
  }
}
